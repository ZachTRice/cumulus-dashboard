    "object-path": "^0.11.3",
    "paginator": "^1.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.2",
    "react-ace": "^4.1.5",
    "react-autocomplete": "^1.7.1",
    "react-collapsible": "^2.0.4",
    "react-dom": "^15.6.2",
