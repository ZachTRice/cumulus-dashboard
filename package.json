    "gulp-load-plugins": "^1.2.2",
    "gulp-plumber": "^1.1.0",
    "gulp-preprocess": "^2.0.0",
    "gulp-rev": "^7.0.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^2.3.1",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^3.0.1",
    "gulp-useref": "^3.1.0",
    "gulp-util": "^3.0.7",
    "node-bourbon": "^4.2.8",
    "node-notifier": "^4.5.0",
    "node-sass": "^3.7.0",
    "react-test-renderer": "^15.6.2",
    "redux-test-utils": "^0.2.2",
    "require-globify": "^1.3.0",
    "tap-xunit": "^1.7.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
