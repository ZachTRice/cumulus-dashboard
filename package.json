{
  "name": "cumulus-dashboard",
  "version": "1.2.0",
  "description": "A dashboard for Cumulus API",
  "repository": {
    "type": "git",
    "url": "https://github.com/cumulus-nasa/cumulus-dashboard"
  },
  "author": {
    "name": "Development Seed",
    "url": "https://developmentseed.org"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cumulus-nasa/cumulus-dashboard/issues"
  },
  "homepage": "https://github.com/cumulus-nasa/cumulus-dashboard",
  "scripts": {
    "serve": "gulp serve",
    "build": "gulp",
    "lint": "eslint app/scripts/ --ext .js",
    "start": "npm run serve",
    "cypress": "CYPRESS_TESTING=true cypress open",
    "cypress-ci": "CYPRESS_TESTING=true cypress run --spec 'cypress/integration/*' --config video=false",
    "validation": "ava cypress/validation-tests/*.js",
    "test": "npm run lint && ava"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/fake-api-db/*.js"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ],
      "require-globify",
      "envify"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.0-rc.1",
    "ava-http": "^1.0.0",
    "babel-core": "^7.0.0-bridge",
    "babelify": "^9.0.0",
    "body-parser": "^1.18.3",
    "browser-sync": "^2.12.5",
    "browserify": "^13.0.1",
    "cls-bluebird": "^2.1.0",
    "cypress": "^3.1.0",
    "del": "^2.2.0",
    "envify": "^3.4.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^1.3.2",
    "express": "^4.16.3",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9.1",
    "gulp-cache": "^0.4.4",
    "gulp-csso": "^2.0.0",
    "gulp-exit": "0.0.2",
    "gulp-if": "^2.0.1",
